group '{{ group }}'
version '{{ version }}'

apply plugin: 'java'
apply plugin: 'idea'
apply plugin: 'eclipse'

sourceCompatibility = 1.8

repositories {
    mavenCentral()
    mavenLocal()
}

dependencies {
    compile group: 'com.navent.swagger-client', name: 'implementation', version: '0.0.1-SNAPSHOT'

    // Jackson
    compile group: 'com.fasterxml.jackson.core', name: 'jackson-annotations', version: '2.9.5'

    testCompile group: 'junit', name: 'junit', version: '4.12'
}
